{
  "name": "@local/vite-plugin-preact-esp32",
  "version": "0.1.0",
  "git-repository": "https://github.com/isaackoz/vite-plugin-preact-esp32",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "description": "Convert vite powered static websites to a header file to be used on an ESP32 webserver.",
  "keywords": [
    "preact",
    "esp32",
    "espressif",
    "espidf",
    "template",
    "react"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch"
  },
  "author": "Isaac Koczwara",
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vite": "6.x"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.2",
    "ejs": "^3.1.10",
    "rollup": "^4.29.1",
    "mime-types": "^2.1.35",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  }
}